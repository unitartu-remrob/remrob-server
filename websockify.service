[Unit]
Description=Websockify bridge
After=network.target

[Service]
User={$USER}
WorkingDirectory=/home/{$USER}/dev/remrob-server/server
ExecStart=websockify --web ./noVNC --token-plugin=TokenFile --token-source=/home/{$USER}/dev/remrob-server/server/websockify-token.cfg 6085
Restart=always

[Install]
WantedBy=multi-user.target
